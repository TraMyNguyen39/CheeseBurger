<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewData["Title"] - CheeseBurgerWeb</title>
    <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/js/bootstrap.min.js" asp-append-version="true" />
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css
">
    <link rel="stylesheet" href="~/css/base.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/layout-admin.css" asp-append-version="true" />
</head>
<body>
    <header class="header">
        <div class="container-fluid">
            <nav class="header__navbar">
                <a asp-page="" class="logo-link">
                    <img src="~/img/logo_black.png" alt="abc" class="logo-server" style="height: 50px">
                </a>
                <a asp-page="" class="header__navbar-admin">
                    <img src="~/img/AdminHub1.png" alt="abc" class="logo-brand" style="height: 25px">
                </a>
                <a asp-page="">
                </a>
            </nav>
        </div>
    </header>
    <div class="background">
        @RenderBody();
    </div>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
